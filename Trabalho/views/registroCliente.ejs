<!doctype html>
<html lang="en">

  <%- include('head'); %>

  <body>
    <section class="container-fluid">
      <section class = "row justify-content-center">
        <section class = "col-12 col-sm-6 col-md-3">
          <section class = "form-container">
              <div class="mt-5">


                <form method="POST" action="/cadastro">

                  <div class="mb-3">
                    <label for="inputNome" class="form-label">Nome</label>
                    <input type="" class="form-control" name="inputNome" id="inputNome">
                  </div>

                  <div class="mb-3">
                    <label for="inputEmail" class="form-label">Email</label>
                    <input type="email" class="form-control" name="inputEmail" id="inputEmail">
                  </div>

                  <div class="mb-3">
                    <label for="inputSenha" class="form-label">Senha</label>
                    <input type="password" class="form-control" name="inputSenha" id="inputSenha">
                  </div>

                  <div class="mb-3">
                    <label for="inputCartao" class="form-label">Numero do cartao de credito</label>
                    <input type="" class="form-control" name="inputCartao" id="inputCartao">
                  </div>

                  
                  <div class="mb-3">
                    <label for="inputPhone" class="form-label">Numero de celular</label>
                    <input type="" class="form-control" name="inputPhone" id="inputPhone">
                  </div>

                  <button type="submit" class="btn btn-primary">Registrar conta</button>
                </form>


              </div>
          </section>
        </section>
      </section>
    </section>

    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
  </body>
</html>